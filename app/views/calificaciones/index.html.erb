<p id="notice"><%= notice %></p>

<h1>Calificaciones</h1>

<table>
  <thead>
    <tr>
      <th>Corte1</th>
      <th>Corte2</th>
      <th>Corte3</th>
      <th>Final</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calificaciones.each do |calificacione| %>
      <tr>
        <td><%= calificacione.corte1 %></td>
        <td><%= calificacione.corte2 %></td>
        <td><%= calificacione.corte3 %></td>
        <td><%= calificacione.final %></td>
        <td><%= link_to 'Show', calificacione %></td>
        <td><%= link_to 'Edit', edit_calificacione_path(calificacione) %></td>
        <td><%= link_to 'Destroy', calificacione, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calificacione', new_calificacione_path %>
